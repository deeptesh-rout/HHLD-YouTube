generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model VideoData {
  id          Int     @id @default(autoincrement())
  title       String
  description String?
  author      String
  url         String
}
